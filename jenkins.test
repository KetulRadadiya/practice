  "type": "object",
  "properties": {
    "ami_id": {
      "type": "string",
      "pattern": "^ami-[a-f0-9]{8,17}$"
    },
    "ssm_param_name": {
      "type": "string"
    },
    "stack_name": {
      "type": "string"
    }
  },
